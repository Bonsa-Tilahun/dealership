<% if @image.persisted? %>
  $('#upload_<%=params[:unique_id]%>').remove();
  $("#uploads_container<%= '_primary' if @vehicle.primary_image == @image %>").append("<%= j(render @image) %>");
<% else %>
  $('#upload_<%=params[:unique_id]%> div.progress').removeClass('active progress-striped').addClass('progress-danger');
<% end %>